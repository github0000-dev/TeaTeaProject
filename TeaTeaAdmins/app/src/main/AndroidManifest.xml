<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teateaadmins">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TeaTeaAdmins">
        <activity android:name=".ViewDelivererActivity"></activity>
        <activity
            android:name=".ViewCustomerActivity"
            android:launchMode="singleInstance"
            android:parentActivityName=".MainMenu" />
        <activity
            android:name=".MainMenu"
            android:label="@string/title_activity_main_main"
            android:theme="@style/Theme.TeaTeaAdmins.NoActionBar" />
        <activity
            android:name=".signupActivity"
            android:launchMode="singleInstance"
            android:parentActivityName=".loginActivity" />
        <activity android:name=".loginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>